{
  "rewrites": [
    {
      "source": "/politica",
      "destination": "/politica.html"
    },
    {
      "source": "/saude",
      "destination": "/saude.html"
    },
    {
      "source": "/agronegocio",
      "destination": "/agronegocio.html"
    },
    {
      "source": "/turismo",
      "destination": "/turismo.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
