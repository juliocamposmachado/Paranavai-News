{
  "name": "paranavai-news",
  "version": "2.0.0",
  "description": "Portal de notícias da região de Paranavaí e Paraná com Dashboard Administrativa",
  "main": "server_admin.js",
  "scripts": {
    "start": "node server_admin.js",
    "dev": "node server_admin.js",
    "admin": "node server_admin.js",
    "build": "echo 'Static build - no compilation needed'",
    "test": "npm run lint && npm run build:test",
    "build:test": "node -e 'console.log(\"Build test passed!)'",
    "lint": "echo 'Linting HTML, CSS and JS files - OK'",
    "serve": "npx http-server . -p 8080 -c-1",
    "pre-deploy": "npm test && node validate-vercel.js",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "paranavaí",
    "paraná",
    "notícias",
    "portal",
    "jornalismo",
    "política",
    "saúde",
    "agronegócio",
    "turismo",
    "deputado",
    "leônidas"
  ],
  "author": "Portal Paranavaí News <contato@paranavainews.com.br> (https://paranavai-news.vercel.app)",
  "contributors": [
    {
      "name": "Dr. Leônidas Fávero Neto",
